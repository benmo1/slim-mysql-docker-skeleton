{
  "name": "morris-php/slim-mysql-docker-skeleton",
  "description": "Skeleton for developing restful apis with slim framework and mysql",
  "keywords": [
    "rest",
    "psr7"
  ],
  "authors": [
    {
      "name": "Ben Morris",
      "email": "benjamin.morris77@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-pdo": "*",
    "monolog/monolog": "^1.17",
    "php": ">=7.1",
    "slim/php-view": "^2.0",
    "slim/slim": "^3.1",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0"
  },
  "autoload": {
    "psr-4": {
      "MorrisPhp\\YouTubeApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
